/dts-v1/;

#include "msm8953-xiaomi-common.dtsi"
#include <dt-bindings/input/atmel-maxtouch.h>
#include <dt-bindings/leds/common.h>

/delete-node/ &qseecom_mem;

/ {
	model = "Xiaomi Redmi 4 Prime";
	compatible = "xiaomi,markw", "qcom,msm8953";
	qcom,board-id = <0x1000b 1>;

	speaker_amp: audio-amplifier {
		compatible = "awinic,aw8738";
		mode-gpios = <&tlmm 96 GPIO_ACTIVE_HIGH>;
		awinic,mode = <5>;
		sound-name-prefix = "Speaker Amp";
	};

	chosen {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		framebuffer0: framebuffer@90001000 {
			compatible = "simple-framebuffer";
			reg = <0 0x90001000 0 (1920 * 1080 * 3)>;

			width = <1080>;
			height = <1920>;
			stride = <(1080 * 3)>;
			format = "r8g8b8";

			power-domains = <&gcc MDSS_GDSC>;

			clocks = <&gcc GCC_MDSS_AHB_CLK>,
				 <&gcc GCC_MDSS_AXI_CLK>,
				 <&gcc GCC_MDSS_VSYNC_CLK>,
				 <&gcc GCC_MDSS_MDP_CLK>,
				 <&gcc GCC_MDSS_BYTE0_CLK>,
				 <&gcc GCC_MDSS_PCLK0_CLK>,
				 <&gcc GCC_MDSS_ESC0_CLK>;
		};
	};

	gpio-keys {
		pinctrl-0 = <&gpio_key_default>;
		pinctrl-names = "default";
	};

	reserved-memory {
		/delete-node/memory@85b00000;

		/delete-node/memory@90001000;
		cont_splash_mem: cont-splash@90001000 {
			reg = <0 0x90001000 0 (1080 * 1920 * 3)>;
			no-map;
		};

		/delete-node/memory@85b00000;
		qseecom_mem: qseecom@84a00000 {
			reg = <0x0 0x84a00000 0x0 0x1900000>;
			no-map;
		};

		ramoops@9ff00000 {
			compatible = "ramoops";
			reg = <0x0 0x9ff00000 0x0 0x00100000>;
			record-size = <0x1000>;
			console-size = <0x80000>;
			ftrace-size = <0x1000>;
			pmsg-size = <0x8000>;
		};
	};

	sensors: i2c-gpio {
		compatible = "i2c-gpio";
		sda-gpios = <&tlmm 14 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		scl-gpios = <&tlmm 15 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		i2c-gpio,delay-us = <2>;        /* ~100 kHz */
		#address-cells = <1>;
		#size-cells = <0>;

		light@23 {
			compatible = "liteon,ltr559";
			reg = <0x23>;

			vdd-supply = <&pm8953_l17>;
			vddio-supply = <&pm8953_l6>;
		};

		magnetometer@2e {
			compatible = "yamaha,yas537";
			reg = <0x2e>;

			vdd-supply = <&pm8953_l6>;
		};

		imu@68 {
			compatible = "bosch,bmi120";
			reg = <0x68>;

			vdd-supply = <&pm8953_l10>;
			vddio-supply = <&pm8953_l6>;

			mount-matrix = "0", "-1", "0",
					"-1", "0", "0",
					"0", "0", "1";
		};
	};
};

&aw2013_led {
	status = "okay";

	led@0 {
		reg = <0>;
		led-max-microamp = <5000>;
		function = LED_FUNCTION_INDICATOR;
		color = <LED_COLOR_ID_RED>;
	};

	led@1 {
		reg = <1>;
		color = <LED_COLOR_ID_GREEN>;
		function = LED_FUNCTION_INDICATOR;
		led-max-microamp = <5000>;
	};

	led@2 {
		reg = <2>;
		color = <LED_COLOR_ID_BLUE>;
		function = LED_FUNCTION_INDICATOR;
		led-max-microamp = <5000>;
	};
};

&battery {
	charge-full-design-microamp-hours = <4000000>;
	constant-charge-current-max-microamp = <1000000>;
	voltage-min-design-microvolt = <3400000>;
	voltage-max-design-microvolt = <4380000>;
};

&camss {
	status = "okay";
	vdda-supply = <&pm8953_s3>;

	ports {
		port@0 {
			reg = <0>;
			csiphy0_ep: endpoint {
				clock-lanes = <7>;
				data-lanes = <0 1 2 3>;
				remote-endpoint = <&rear_cam_ep>;
			};
		};
		port@2 {
			reg = <2>;
			csiphy2_ep: endpoint {
				clock-lanes = <7>;
				data-lanes = <0 1>;
				remote-endpoint = <&front_cam_ep>;
			};
		};
	};
};

&cci {
	pinctrl-names = "default";
	pinctrl-0 = <&cci0_default>,
		    <&cci1_default>,
		    <&camss_mclk0_default>,
		    <&camss_mclk1_default>;

	status = "okay";
};

&cci_i2c0 {
	camera-sensor@2d { // Primary rear camera
		compatible = "samsung,s5k3l8";

		reg = <0x2d>;

		clocks = <&gcc GCC_CAMSS_MCLK0_CLK>;
		clock-frequency = <24000000>;

		reset-gpios = <&tlmm 40 GPIO_ACTIVE_LOW>;

		avdd-supply= <&pm8953_l22>;
		dvdd-supply = <&pm8953_l2>;
		vio-supply = <&pm8953_l6>;
		aux-supply = <&pm8953_l17>;

		rotation = <270>;
		orientation = <1>;

		status = "okay";

		port {
			rear_cam_ep: endpoint {
				data-lanes = <0 1 2 3>;
				remote-endpoint = <&csiphy0_ep>;
			};
		};
	};
};

&cci_i2c1 {
	camera-sensor@36 { // Front camera
		compatible = "ovti,ov5670";

		reg = <0x36>;

		clocks = <&gcc GCC_CAMSS_MCLK1_CLK>;
		clock-rates = <19200000>;

		pinctrl-0 = <&cam_sensor_front_default>;
		pinctrl-names = "default";

		reset-gpios = <&tlmm 129 GPIO_ACTIVE_LOW>;

		avdd-supply= <&pm8953_l22>;
		dvdd-supply = <&pm8953_l23>;
		dovdd-supply = <&pm8953_l6>;
		orientation = <0>;
		rotation = <270>;

		status = "okay";

		port {
			front_cam_ep: endpoint {
				data-lanes = <0 1>;
				remote-endpoint = <&csiphy2_ep>;
				link-frequencies = /bits/ 64 <450000000>;
			};
		};
	};
};

&ft5406_ts {
	status = "disabled";

	compatible = "edt,edt-ft5336", "edt,edt-ft5306";
	touchscreen-size-x = <1080>;
	touchscreen-size-y = <1920>;
};

&i2c_3 {
	status = "okay";

	touchscreen@4a {
		compatible = "atmel,maxtouch";
		reg = <0x4a>;

		interrupt-parent = <&tlmm>;
		interrupts = <65 IRQ_TYPE_EDGE_FALLING>;

		pinctrl-0 = <&ts_reset_active &ts_int_active>;
		pinctrl-1 = <&ts_reset_suspend &ts_int_suspend>;
		pinctrl-names = "default", "sleep";

		reset-gpios = <&tlmm 64 GPIO_ACTIVE_LOW>;

		vdda-supply = <&pm8953_l5>;
		vdd-supply = <&pm8953_l10>;

		atmel,wakeup-method = <ATMEL_MXT_WAKEUP_I2C_SCL>;
		status = "disabled";
	};
};

&panel {
	compatible = "xiaomi,markw-panel";
};

&pmi8950_flash {
	status = "okay";

	led@0 {
		reg = <0>;
		function = LED_FUNCTION_FLASH;
		color = <LED_COLOR_ID_WHITE>;
		led-max-microamp = <200000>;
		flash-max-microamp = <1000000>;
		flash-max-timeout-us = <1280000>;
	};

	led@1 {
		reg = <1>;
		function = LED_FUNCTION_FLASH;
		color = <LED_COLOR_ID_YELLOW>;
		led-max-microamp = <200000>;
		flash-max-microamp = <1000000>;
		flash-max-timeout-us = <1280000>;
	};
};

&pm8953_l2 {
	regulator-min-microvolt = <1200000>;
};

&pmi8950_haptics {
	qcom,actuator-type = <HAP_TYPE_ERM>;
	qcom,brake-pattern = <0x3 0x3 0x0 0x0>;
	qcom,wave-play-rate-us = <5263>;
	qcom,wave-shape = <HAP_WAVE_SQUARE>;

	status = "okay";
};

&sound_card {
	model = "xiaomi-markw";

	aux-devs = <&speaker_amp>;
	audio-routing =
		"AMIC1", "MIC BIAS External1",
		"AMIC2", "MIC BIAS Internal2",
		"AMIC3", "MIC BIAS External1",
		"MM_DL1", "MultiMedia1 Playback",
		"MM_DL3", "MultiMedia3 Playback",
		"MM_DL4", "MultiMedia4 Playback",
		"MultiMedia2 Capture", "MM_UL2";

	pinctrl-0 = <&cdc_pdm_lines_act &cdc_pdm_lines_2_act &cdc_pdm_comp_lines_act>;
	pinctrl-1 = <&cdc_pdm_lines_sus &cdc_pdm_lines_2_sus &cdc_pdm_comp_lines_sus>;
	pinctrl-names = "default", "sleep";

	status = "okay";
};

&tlmm {
	gpio-reserved-ranges = <0 4>, <135 4>;

	cam_sensor_front_default: cam_sensor_front_default {
		pins = "gpio129";
		function = "gpio";
		bias-disable;
		drive-strength = <2>;
	};
};

&wcd_codec {
	/delete-property/ qcom,gnd-jack-type-normally-open;
	qcom,mbhc-vthreshold-high = <75 225 450 510 540>;
	qcom,mbhc-vthreshold-low = <25 200 325 500 530>;
};

&zap_shader {
	firmware-name = "qcom/msm8953/xiaomi/markw/a506_zap.mdt";
};
